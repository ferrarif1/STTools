<view style="width:100%;height:100%;background-color:white;min-height:100vh;">
    <view class="bgDiv _div"></view>
    <button bindtap="__e" class="loginBtn gray" data-event-opts="{{[ [ 'tap',[ [ 'showTip',['$event'] ] ] ] ]}}" hidden="{{!!check}}">手机号一键登录</button>
    <button bindgetphonenumber="__e" class="loginBtn" data-event-opts="{{[ [ 'getphonenumber',[ [ 'getPhoneNumber',['$event'] ] ] ] ]}}" hidden="{{!check}}" openType="getPhoneNumber">手机号一键登录</button>
    <view class="aggP _div">
        <label bindtap="__e" class="{{['uncheck','_span',check?'checked':'']}}" data-event-opts="{{[ [ 'tap',[ [ 'e0',['$event'] ] ] ] ]}}"></label>
        <view class="_p">我已阅读并同意<label bindtap="__e" class="_span" data-event-opts="{{[ [ 'tap',[ [ 'goto',['YS'] ] ] ] ]}}" style="color:rgba(0, 204, 166, 1);">《杭州市互联网医院隐私权政策》</label>
            <label bindtap="__e" class="_span" data-event-opts="{{[ [ 'tap',[ [ 'goto',['ET'] ] ] ] ]}}" style="color:rgba(0, 204, 166, 1);">《儿童隐私政策》</label>
        </view>
    </view>
</view>
