.toolbar {
    align-items: center;
    bottom: 0;
    display: flex;
    height: 80px;
    margin: 20px auto;
    position: fixed;
    width: 100%;
}

.pushViewBox {
    height: 100vh;
    width: 100vw;
}

.pushView {
    background-color: hsla(0,0%,100%,.1);
    border-radius: 10px;
    height: 148px;
    padding: 8px;
    right: 0;
    width: 103px;
    z-index: 1;
}

.pullView,.pushView {
    position: fixed;
    top: 0;
}

.pullView {
    align-items: center;
    display: flex;
    height: 100vh;
    left: 0;
    width: 100vw;
}

.uid {
    color: green;
    position: absolute;
    top: 0;
    z-index: 1;
}

.hangup {
    background: url(data:image/png;base64,) no-repeat 50%;
    background-size: 100%;
    border: none;
    border-radius: 36px;
    height: 72px;
    width: 72px;
}

.camera {
    background: url(data:image/png;base64,) no-repeat 50%;
    background-size: 100%;
}

.camera,.mute {
    border: none;
    border-radius: 30px;
    height: 53px;
    width: 53px;
}

.mute {
    background: url(data:image/png;base64,) no-repeat 50%;
    background-size: 100%;
}

.mute.jy {
    background: url(data:image/png;base64,) no-repeat 50%;
    background-size: 100%;
}

.toolbarR {
    bottom: 40vh;
    height: auto;
    margin-right: 20px;
    position: fixed;
    right: 0;
}

.barIt,.toolbarR {
    align-items: center;
    display: flex;
    flex-direction: column;
}

.barIt {
    color: #fff;
    font-size: 11px;
    font-weight: 600;
    margin-bottom: 20px;
}

.barIt ._div {
    height: 27px;
    margin-bottom: 5px;
    width: 27px;
}

.full {
    background: url(data:image/png;base64,) no-repeat 50%;
    background-size: 100%;
}

.full.contain {
    background: url(data:image/png;base64,) no-repeat 50%;
    background-size: 100%;
}

.fz {
    background: url(data:image/png;base64,) no-repeat 50%;
    background-size: 100%;
}